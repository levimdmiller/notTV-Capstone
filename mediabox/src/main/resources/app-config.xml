<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:mvc="http://www.springframework.org/schema/mvc"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
  http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
  http://www.springframework.org/schema/context
  http://www.springframework.org/schema/context/spring-context-2.5.xsd
  http://www.springframework.org/schema/mvc http://www.springframework.org/schema/mvc/spring-mvc.xsd">

  <context:annotation-config />

  <bean id="PortMapper" class="filesharingsystem.ClingPortMapper">
    <constructor-arg index="0" value="1234"/>
  </bean>
  
  <bean id="Config" class="springbackend.Config">
    <!-- <property name="serverUrl" value="http://nottv.levimiller.ca"/> -->
    <property name="serverUrl" value="localhost:1248"/>
    <property name="trackerUrl" value="http://nottv.levimiller.ca:6969"/>
  </bean>

  <mvc:interceptors>
    <bean class="springbackend.ConfigHandlerInterceptor"/>
  </mvc:interceptors>
</beans>
